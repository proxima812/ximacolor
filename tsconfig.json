{
	"extends": "astro/tsconfigs/base",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"strict": false,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"checkJs": false,
		"skipLibCheck": true,
		"allowJs": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "solid-js"
	}
}
